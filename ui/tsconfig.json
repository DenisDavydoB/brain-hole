{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "target": "ESNext",
    "lib": ["DOM", "DOM.Iterable", "ESNext"],
    "module": "ESNext",
    "moduleResolution": "NodeNext",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "baseUrl": "./",
    "paths": {
      "@/*": ["src/*"]
    }
  },
  "include": ["src", "src/components/organism/Main/model/store.ts", "src/components/organism/Main/model/helpers.ts", "src/components/organism/Main/model/types.ts"],
  "references": [{ "path": "./tsconfig.node.json" }]
}
